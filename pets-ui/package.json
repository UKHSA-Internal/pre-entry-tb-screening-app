{
  "name": "pets-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --quiet .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "test-coverage": "vitest run --coverage",
    "cypress:open": "cypress open -C cypress/cypress.config.mjs",
    "cypress:run": "cypress run -C cypress/cypress.config.mjs",
    "cypress:open:qat": "cross-env ENVIRONMENT=qat cypress open -C cypress/cypress.config.mjs",
    "cypress:open:dev": "cross-env ENVIRONMENT=dev cypress open -C cypress/cypress.config.mjs",
    "cypress:open:preprod": "cross-env ENVIRONMENT=preprod cypress open -C cypress/cypress.config.mjs",
    "cypress:run:qat": "cross-env ENVIRONMENT=qat cypress run -C cypress/cypress.config.mjs",
    "cypress:run:dev": "cross-env ENVIRONMENT=dev cypress run -C cypress/cypress.config.mjs",
    "cypress:run:preprod": "cross-env ENVIRONMENT=preprod cypress run -C cypress/cypress.config.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.864.0",
