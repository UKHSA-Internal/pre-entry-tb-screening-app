    "react-hook-form": "^7.62.0",
    "react-redux": "^9.2.0",
    "react-router": "^6.28.0",
    "react-router-dom": "^6.28.0",
    "sass": "^1.81.0",
    "swagger-ui-react": "^5.28.0"
